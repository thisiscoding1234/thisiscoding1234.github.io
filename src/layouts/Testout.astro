---
interface Props {
	title: string;
}

const { title } = Astro.props;
import '../styles/global.css'
import Navbar from '../components/Navbar.astro';
---

<!doctype html>
<html lang="en">
	<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Thisiscoding1234 is testing: {title}</title>
    <meta name="description" content="just a simple website that does nothing and achives nothing" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="apple-touch-icon" sizes="180x180" href="/img/icons/apple-touch-icon.png" />
    <link rel="icon" type="image/png" sizes="32x32" href="/img/icons/favicon-32x32.png" />
    <link rel="icon" type="image/png" sizes="16x16" href="/img/icons/favicon-16x16.png" />
    <link rel="manifest" href="/img/icons/site.webmanifest" />
    <link rel="mask-icon" href="/img/icons/safari-pinned-tab.svg" color="#ff6a00" />
    <link rel="shortcut icon" href="/img/icons/favicon.ico" />
    <meta name="msapplication-TileColor" content="#da532c" />
    <meta name="msapplication-config" content="/img/icons/browserconfig.xml" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <meta name="theme-color" content="#ffffff" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Outfit:wght@100..900&family=Raleway:ital,wght@0,100..900;1,100..900&family=Sixtyfour+Convergence:BLED,SCAN,XELA,YELA@0..100,-53..100,-100..100,-100..100&display=swap" rel="stylesheet">
	</head>
	<body>
        <Navbar></Navbar>
		<slot />
	</body>
</html>
